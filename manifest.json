{ "browser_action" : { "default_icon" : "icons/icon16.png"},
  "manifest_version": 2,
  "description" : "Alert on Google Load",
  "icons" : { "128" : "icons/wdi_128.png",
               "48" : "icons/wdi_48.png",
               "32" : "icons/wdi_32.png",
               "16" : "icons/wdi_16.png" },
  "name" : "Schoolboy",
  "web_accessible_resources": ["icons/wdi.png"],
  "version" : "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["css/style.css"],
      "js": ["js/jquery.js", "js/utils.js", "src/bg/background.js"],
      "run_at": "document_end"
    }
  ],
  "default_locale": "en"
}



